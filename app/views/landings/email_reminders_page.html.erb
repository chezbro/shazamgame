<div class="container">
  <h1>Send email</h1>
  <ul>
    <h4>Users that haven't made All Selections:</h4>
    <% @email_list.each do |email| %>
      <li style="list-style-type:none;">
        <%= email %>
      </li>
    <% end %>
  </ul>

  <%= button_to 'Send Email to these Users', send_reminder_email_path, method: :post, class: 'btn btn-primary'%>

</div>
